            "ctrl-f ctrl-3": "editor::FoldAtLevel_3"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "ctrl-f ctrl-4": "editor::FoldAtLevel_4"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "ctrl-f ctrl-5": "editor::FoldAtLevel_5"
        }
